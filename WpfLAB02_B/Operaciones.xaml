        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfLAB02_B"
        mc:Ignorable="d"
        Title="Operaciones" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="200">
            <Button x:Name="btnOpIngresos" Content="Ingresos" Height="40" Click="btnOpIngresos_Click"/>
            <Button x:Name="btnOpSalida" Content="Salidas" Height="40" Click="btnOpSalida_Click"/>
        </StackPanel>
    </Grid>
</Window>
