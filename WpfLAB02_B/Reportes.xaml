<Window x:Class="WpfLAB02_B.Reportes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfLAB02_B"
        mc:Ignorable="d"
        Title="Reportes" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="200">
            <Button x:Name="btnCargas" Content="Cargas" Height="40" Click="btnCargas_Click"/>
            <Button x:Name="btnIngresos" Content="Ingresos" Height="40" Click="btnIngresos_Click"/>
            <Button x:Name="btnSalidas" Content="Salidas" Height="40" Click="btnSalidas_Click"/>
        </StackPanel>

    </Grid>
</Window>
